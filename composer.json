{
    "name": "renoki-co/laravel-steampipe",
    "description": "Use Laravel's built-in ORM classes to query cloud resources with Steampipe.",
    "keywords": [
        "laravel",
        "php",
        "steampipe",
        "aws",
        "gcp",
        "azure",
        "alibaba",
        "orm",
        "cloud",
        "sql",
        "query"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-steampipe",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/console": "^8.83|^9.0.1",
        "illuminate/database": "^8.83|^9.0.1",
        "illuminate/support": "^8.83|^9.0.1",
        "symfony/process": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelSteampipe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelSteampipe\\Test\\": "tests",
            "App\\": "vendor/orchestra/testbench-core/laravel/app/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.28|^7.0",
        "orchestra/testbench-core": "^6.28|^7.0",
        "phpunit/phpunit": "^9.5.14"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelSteampipe\\LaravelSteampipeServiceProvider"
            ]
        }
    }
}
