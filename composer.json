{
    "name": "renoki-co/laravel-steampipe",
    "description": "Use Laravel's built-in ORM classes to query cloud resources with Steampipe.",
    "keywords": ["laravel", "php", "steampipe", "aws", "gcp", "azure", "alibaba", "orm", "cloud", "sql", "query"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-steampipe",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\LaravelSteampipe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\LaravelSteampipe\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\LaravelSteampipe\\LaravelSteampipeServiceProvider"
            ]
        }
    }
}
